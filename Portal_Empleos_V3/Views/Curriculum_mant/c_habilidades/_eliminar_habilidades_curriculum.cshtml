<md-dialog>

    <md-toolbar>
        <div class="md-toolbar-tools">

        </div>
    </md-toolbar>

    <md-dialog-content>
        <div class="lg-dialog-content">


                    <form name="mainForm" data-ng-submit="eliminar_habilidad()" data-ng-controller="data1" ng-model="elimina_habilidad" novalidate>
                        <div class="text-center well">
                            <h1 class="text-center">Eliminar Competencia</h1>
                            <ul class="list-group">
                                <li class="list-group-item">
                                    <h3>Desea eliminar esta Competencia:</h3>
                                </li>
                            
                                        @Html.Hidden("id_habilidad_curriculum", (Int32)ViewBag.id_habilidad, new { @ng_model = "elimina_habilidad.id", ng_init = "elimina_habilidad.id = " + (Int32)ViewBag.id_habilidad })
                                  
                                <li class="list-group-item">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <button type="submit" class="btn btn-block btn-danger btn-lg">Eliminar</button>


                                        </div>
                                    </div>
                                </li>
                                <li class="list-group-item">
                                    <div class="row">
                                        <div class="col-md-12">


                                            <div class="form-group ">
                                                <div ng-controller="data1">

                                                    <a href="" class="btn  btn-block btn-info btn-lg" ng-click="closeDialog()">Cancelar</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </form>
                </div>
   
    </md-dialog-content>
</md-dialog>